@model IList<MurvasBokhandel.Controllers.BookResult>

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>
<div class="row">
    <div class="col-md-4">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Författare</th>
                    <th></th>
                    <th>Boktitel</th>
                </tr>
            </thead> 
            <tbody>
                @foreach (var book in Model)
                {
                    <tr>
                        <td>@book.FirstName</td>
                        <td>@book.LastName</td>
                        <td>@book.Title</td> 
                    </tr>
                }
            </tbody>
            
        </table>
    </div>
</div>

